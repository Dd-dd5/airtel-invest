# Production Environment Variables for TrueHost
# Copy these to your actual .env file and fill in real values

# M-Pesa Configuration (Get from Safaricom Developer Portal)
VITE_MPESA_API_URL=https://api.safaricom.co.ke
VITE_MPESA_SHORTCODE=your_actual_shortcode
VITE_MPESA_PASSKEY=your_actual_passkey
VITE_MPESA_CONSUMER_KEY=your_actual_consumer_key
VITE_MPESA_CONSUMER_SECRET=your_actual_consumer_secret

# Airtel Money Configuration
VITE_AIRTEL_API_URL=https://openapi.airtel.africa/merchant/v1/payments/
VITE_AIRTEL_CLIENT_ID=your_airtel_client_id
VITE_AIRTEL_CLIENT_SECRET=your_airtel_client_secret

# Admin Configuration
VITE_ADMIN_ACCESS_KEY=your_secure_admin_key_2024
VITE_ADMIN_DOMAINS=yourdomain.com,www.yourdomain.com,admin.yourdomain.com